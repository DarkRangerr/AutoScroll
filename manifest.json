{
  "name": "AutoScroll",
  "version": "1.0",
  "description": "Autoscrolling for the lazy!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/scroll.png"
    }
  },
  "icons": {
      "128": "images/scroll.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}